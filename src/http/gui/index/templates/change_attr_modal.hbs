{{#modal-dialog title='Rename file' ok='changeAttr' close='hideModal'}}
    <form {{action 'ok' on='submit' target=view}}>
        <div class="form-group">
            {{input
            type='text'
            id='file-attr'
            placeholder='New attribute'
            value=currentFileAttr
            autofocus='autofocus'}}
        </div>
    </form>
{{/modal-dialog}}