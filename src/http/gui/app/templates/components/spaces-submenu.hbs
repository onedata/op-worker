<ul class="submenu">

    {{#each items as |item|}}
      <li class="{{item.type}}-permissions">
        <div {{action 'changeActiveOption' item.type}}
          class="secondary-sidebar-item">
          <span class="item-element item-click-area">
            <span class="item-icon">
              {{icon item.icon 'color-space'}}
            </span>
            <span class="item-label">
              {{t item.labelI18n}}
            </span>
          </span>
        </div>
      </li>
    {{/each}}

</ul>
